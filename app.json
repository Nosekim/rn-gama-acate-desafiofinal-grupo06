{
  "expo": {
    "name": "Find Dev",
    "slug": "find-dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#272629"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.acategama.finddev.grupo06"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#272629"
      },
      "package": "com.acategama.finddev.grupo06",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Precisamos de sua permissão para acessar a sua galeria de imagens para que você possa selecionar uma imagem para o seu perfil.",
          "cameraPermission": "Precisamos de sua permissão para acessar a sua câmera para que você possa tirar uma foto para adicionar ao seu perfil."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notification-icon.png",
          "color": "#272629"
        }
      ]
    ]
  }
}
